{
  "manifest_version": 2,
  "name": "Google Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
    "matches": [
      "<all_urls>"
      ],
    "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "yoda.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions":["storage", "alarms", "notifications"]
}
